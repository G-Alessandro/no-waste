generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserLocations {
  id       Int        @id @default(autoincrement())
  userId   Int
  location Location[]
}

model Location {
  id              Int           @id @default(autoincrement())
  name            String        @db.VarChar(30)
  latitude        Float
  longitude       Float
  userLocations   UserLocations @relation(fields: [userLocationsId], references: [id])
  userLocationsId Int
}
